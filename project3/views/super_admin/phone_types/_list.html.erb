<table class="table table-striped table-bordered table-hover" id="super_phone_types">
  <thead>
  <tr>
    <th>
      Phone Type
    </th>
    <th>
      Description
    </th>
    <th>
      Action
    </th>
  </tr>
  </thead>
  <% unless @phone_types.blank? %>
      <tbody>
      <% @phone_types.each_with_index do |phone_type, index| %>
          <tr class="odd gradeX">
            <td>
              <%= phone_type.name %>
            </td>
            <td>
              <%= phone_type.description %>
            </td>
            <td>
              &nbsp;&nbsp;&nbsp;
              <%= link_to "Edit", edit_super_admin_phone_type_path(phone_type.id), :style => "text-decoration:none" %>
              &nbsp;&nbsp;&nbsp;

              <%= link_to "View", super_admin_phone_type_path(phone_type.id), :style => "text-decoration:none" %>
              &nbsp;&nbsp;&nbsp;

            <!--<td>-->
            <!--<span class="label label-sm label-success">-->
            <!--Approved </span>-->
            <!--</td>-->
          </tr>
      <% end %>
      </tbody>
  <% end %>
</table>



